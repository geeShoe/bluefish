{
    "name": "geeshoe/bluefish",
    "description": "PHP user authentication library",
    "keywords": [
        "geeshoe",
        "bluefish",
        "authentication"
    ],
    "homepage": "https://geeshoe.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jesse Rushlow",
            "email": "jr@geeshoe.com",
            "homepage": "https://geeshoe.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "ext-pdo_mysql" : "*",
        "ramsey/uuid": "^3.8",
        "geeshoe/dblib": "@alpha",
        "geeshoe/helpers": "^0.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "symfony/dotenv": "^4.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Geeshoe\\BlueFish\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Geeshoe\\BlueFish\\Tests\\": "tests/"
        }
    }
}
